{
  "name": "elise",
  "version": "2.1.0",
  "description": "rest api made with node, express, and typescript",
  "repository": "https://github.com/archisdi/expRESTful-typed",
  "main": "index.js",
  "scripts": {
    "serve": "nodemon",
    "build": "sh ./scripts/deployment/build.sh",
    "clean-install": "rm -rf node_modules package-lock.json && npm install",
    "unit-test": "NODE_ENV=test nyc ava --verbose",
    "lint": "eslint './src/**/*.{js,ts}' --fix",
    "migrate": "./scripts/migration/migrate.sh",
    "migrate:rollback": "./scripts/migration/rollback.sh",
    "migrate:refresh": "./scripts/migration/refresh.sh",
    "db:seed": "./scripts/migration/seed.sh"
  },
  "author": "Archie Isdiningrat",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/bull": "^3.13.0",
    "@types/chai": "^4.1.7",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/hapi__joi": "^15.0.2",
    "@types/helmet": "0.0.43",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/node": "^12.0.10",
    "@types/randomstring": "^1.1.6",
    "@types/sequelize": "^4.28.3",
    "@types/sinon": "^7.0.13",
    "ava": "^3.8.1",
    "install": "^0.12.2",
    "nodemon": "^1.19.1",
    "npm": "^6.10.1",
    "nyc": "^14.1.1",
    "sequelize-cli": "^5.5.0",
    "sinon": "^7.3.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.9.2",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "eslint": "^7.2.0"
  },
  "dependencies": {
    "@hapi/joi": "^15.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bull": "^3.14.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^15.0.0",
    "helmet": "^3.18.0",
    "http-status-codes": "^1.3.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "randomstring": "^1.1.5",
    "sequelize": "^5.10.1",
    "tymon": "git+https://github.com/archisdi/tymon.git",
    "uuid": "^3.3.2"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register/transpile-only"
    ],
    "files": [
      "./tests/**/*.test.ts"
    ]
  }
}
